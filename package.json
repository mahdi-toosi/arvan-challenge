	"scripts": {
		"prepare": "husky install",
		"dev": "vite",
		"build": "cross-env NODE_ENV=production vue-tsc --noEmit && vite build",
		"build:prod": "cross-env NODE_ENV=production vite build",
		"build:stg": "cross-env NODE_ENV=staging vite build --mode staging",
		"serve": "vite preview",
		"lint": "eslint --ext .js,.ts,.vue .",
		"lint:fix": "yarn lint --fix",
		"lint:markup": "vue-tsc --noEmit",
		"format:check": "prettier .",
		"format:fix": "prettier -u --write .",
		"generate:changelog": "standard-version --skip.commit --skip.tag",
		"release": "standard-version --bumpFiles './manifest.json' --packageFiles './manifest.json' && git push origin main --follow-tags"
	},
